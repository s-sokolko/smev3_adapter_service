<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2006 rel. 3 U (http://www.altova.com) by Company (GNIVC FNS RF) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="urn://x-artefacts-fns-vipul-tosmv-ru/311-14/4.0.6" xmlns:fnst="urn://x-artefacts-fns/vipul-types/4.0.6" targetNamespace="urn://x-artefacts-fns-vipul-tosmv-ru/311-14/4.0.6" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="urn://x-artefacts-fns/vipul-types/4.0.6" schemaLocation="./commons/fns-vipul-types.xsd"/>
	<xs:element name="FNSVipULRequest">
		<xs:annotation>
			<xs:documentation>Запрос на предоставление из ЕГРЮЛ выписки о юридическом лице</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ЗапросЮЛ">
					<xs:annotation>
						<xs:documentation>Идентификационные сведения о юридическом лице, в отношении которого составлен запрос</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="ОГРН" type="fnst:OGRNCompanyType">
								<xs:annotation>
									<xs:documentation>Основной государственный регистрационный номер юридического лица</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ИННЮЛ" type="fnst:LegalPersonINNType">
								<xs:annotation>
									<xs:documentation>ИНН юридического лица</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ИдДок" type="fnst:string-36" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор запроса</xs:documentation>
					<xs:documentation>GUID</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="НомерДела" type="fnst:string-50" use="optional">
				<xs:annotation>
					<xs:documentation>Номер дела, находящегося в производстве в суде или правоохранительном органе, участником которого является юридическое лицо, в отношении которого запрашивается выписка</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FNSVipULResponse">
		<xs:annotation>
			<xs:documentation>Выписка из ЕГРЮЛ в отношении юридического лица</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="СвЮЛ">
					<xs:annotation>
						<xs:documentation>Сведения о  юридическом лице</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвНаимЮЛ">
								<xs:annotation>
									<xs:documentation>Сведения о наименовании юридического лица</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
												<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="НаимЮЛПолн" type="fnst:string-1000" use="required">
										<xs:annotation>
											<xs:documentation>Полное наименование юридического лица на русском языке</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="НаимЮЛСокр" type="fnst:string-510" use="optional">
										<xs:annotation>
											<xs:documentation>Сокращенное наименование юридического лица на русском языке</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвАдресЮЛ">
								<xs:annotation>
									<xs:documentation>Сведения об адресе (месте нахождения)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="АдресРФ" type="fnst:АдрРФЕГРЮЛТип">
											<xs:annotation>
												<xs:documentation>Адрес (место нахождения) юридического лица</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвНедАдресЮЛ" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о недостоверности адреса</xs:documentation>
												<xs:documentation>Формируется только если в ЕГРЮЛ внесены сведения о недостоверности адреса, а сам адрес ЮЛ еще не изменен</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="РешСудНедАдр" type="fnst:РешСудТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сведения о решении суда, на основании которого адрес признан недостоверным</xs:documentation>
															<xs:documentation>Формируется в случае, если значение реквизита ПризнНедАдресЮЛ = 3</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
														<xs:annotation>
															<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
															<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="ПризнНедАдресЮЛ" use="required">
													<xs:annotation>
														<xs:documentation>Признак недостоверности адреса </xs:documentation>
														<xs:documentation>, где:
																	2 - признак недостоверности, внесенный в ЕГРЮЛ по результатам проверки достоверности содержащихся в ЕГРЮЛ сведений о юридическом лице (если сведения о недостоверности сведений об адресе внесены на основании записи с кодом СПВЗ 17023;
3 - признак недостоверности, внесенный в ЕГРЮЛ на основании решения суда (если сведения о недостоверности сведений об адресе внесены на основании записи с кодом СПВЗ 16006)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="fnst:string-1">
															<xs:enumeration value="2"/>
															<xs:enumeration value="3"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ТекстНедАдресЮЛ" use="optional">
													<xs:annotation>
														<xs:documentation>Текст о недостоверности сведений, выводимый в выписке в строке с наименованием «Дополнительные сведения»</xs:documentation>
														<xs:documentation>Принимает значение: 
•	«сведения недостоверны (результаты проверки достоверности содержащихся в ЕГРЮЛ сведений о юридическом лице)» -   если сведения о недостоверности внесены по данным, выявленным НО (ПризнНедАдресЮЛ=2):
•	«сведения недостоверны (решение суда)» -  если сведения о недостоверности внесены на основании решения суда (ПризнНедАдресЮЛ=3).
Выводится в выписке в строке с наименованием «Дополнительные сведения»</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="fnst:string-500"/>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="СвРешИзмМН" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о принятии юридическим лицом решения об изменении места нахождения</xs:documentation>
												<xs:documentation>Формируется только если в ЕГРЮЛ внесены сведения о принятии такого решения, а сам адрес ЮЛ еще не изменен</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Регион" type="fnst:РегионТип">
														<xs:annotation>
															<xs:documentation>Субъект Российской Федерации</xs:documentation>
															<xs:documentation>Выгружаются сведения о субъекте РФ по новому месту нахождения ЮЛ из принятого решения об изменении места нахождения</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Район" type="fnst:РайонТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Район (улус и т.п.)</xs:documentation>
															<xs:documentation>Выгружаются сведения о районе по новому месту нахождения ЮЛ из принятого решения об изменении места нахождения (при наличии в заявлении о принятом решении об изменении места нахождения ЮЛ)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Город" type="fnst:ГородТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Город (волость и т.п.)</xs:documentation>
															<xs:documentation>Выгружаются сведения о городе по новому месту нахождения ЮЛ из принятого решения об изменении места нахождения (при наличии в заявлении о принятом решении об изменении места нахождения ЮЛ)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="НаселПункт" type="fnst:НаселПунктТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Населенный пункт (село и т.п.)</xs:documentation>
															<xs:documentation>Выгружаются сведения о населенном пункте по новому месту нахождения ЮЛ из принятого решения об изменении места нахождения (при наличии в заявлении о принятом решении об изменении места нахождения ЮЛ))</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
														<xs:annotation>
															<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
															<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="ТекстРешИзмМН" use="required">
													<xs:annotation>
														<xs:documentation>Текст, выводимый в выписке в строке с наименованием «Дополнительные сведения»</xs:documentation>
														<xs:documentation>Принимает значение:
																	Юридическим лицом принято решение об изменении места нахождения.
														Выводится в выписке в строке с наименованием «Дополнительные сведения»</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="fnst:string-500"/>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвАдрЭлПочты" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения об адресе электронной почты юридического лица</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической  ошибки в указанных сведениях</xs:documentation>
												<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="E-mail" type="fnst:string-50" use="required">
										<xs:annotation>
											<xs:documentation>Адрес электронной почты</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвОбрЮЛ">
								<xs:annotation>
									<xs:documentation>Сведения о регистрации (образовании) юридического лица</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СпОбрЮЛ">
											<xs:annotation>
												<xs:documentation>Способ образования юридического лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="КодСпОбрЮЛ" type="fnst:string-2" use="required">
													<xs:annotation>
														<xs:documentation>Код способа образования по справочнику СЮЛНД</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="НаимСпОбрЮЛ" type="fnst:string-255" use="optional">
													<xs:annotation>
														<xs:documentation>Наименование способа образования юридического лица</xs:documentation>
														<xs:documentation>Отсутствует в случае, если в реквизите КодСпОбрЮЛ  передается код =«03» (Сведения о регистрации на территории Республики Крым или территории города федерального значения Севастополя на день принятия в Российскую Федерацию Республики Крым и образования в составе Российской Федерации новых субъектов - Республики Крым и города федерального значения Севастополя)</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
												<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ОГРН" type="fnst:OGRNCompanyType" use="required">
										<xs:annotation>
											<xs:documentation>Основной государственный регистрационный номер юридического лица</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаОГРН" use="required">
										<xs:annotation>
											<xs:documentation>Дата присвоения ОГРН</xs:documentation>
											<xs:documentation>Для ЮЛ, созданных после 01.07.2002 г., является датой регистрации ЮЛ</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date"/>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="РегНом" type="fnst:string-255" use="optional">
										<xs:annotation>
											<xs:documentation>Регистрационный номер, присвоенный российскому юридическому лицу до 1 июля 2002 года, или регистрационный номер юридического лица на территории Республики Крым или территории города федерального значения Севастополя на день принятия в РФ и образования в составе РФ новых субъектов - Республики Крым и города федерального значения Севастополя</xs:documentation>
											<xs:documentation>Формируется только в отношении российских ЮЛ, созданных до 1 июля 2002 года, а также в отношении ЮЛ зарегистрированных на территории Республики Крым или территории города федерального значения Севастополя на день принятия в РФ и образования в составе РФ новых субъектов - Республики Крым и города федерального значения Севастополя (значение реквизита «КодСпОбрЮЛ» в элементе «СпОбрЮЛ» =«03»)
											Не формируется в отношении российских организаций, созданных с 1 июля 2002 года (значение реквизита «КодСпОбрЮЛ» в элементе «СпОбрЮЛ» больше «03»)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаРег" use="optional">
										<xs:annotation>
											<xs:documentation>Дата регистрации юридического лица</xs:documentation>
											<xs:documentation>
											Формируется только в отношении российских ЮЛ, созданных до 1 июля 2002 года, а также в отношении ЮЛ зарегистрированных на территории Республики Крым или территории города федерального значения Севастополя на день принятия в РФ и образования в составе РФ новых субъектов - Республики Крым и города федерального значения Севастополя (значение реквизита «КодСпОбрЮЛ» в элементе «СпОбрЮЛ» =«03»)
Не формируется в отношении российских организаций, созданных с 1 июля 2002 года (значение реквизита «КодСпОбрЮЛ» в элементе «СпОбрЮЛ» больше «03»)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date"/>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="НаимРО" type="fnst:string-255" use="optional">
										<xs:annotation>
											<xs:documentation>Наименование органа, зарегистрировавшего юридическое лицо</xs:documentation>
											<xs:documentation>Формируется только в отношении российских ЮЛ, созданных до 1 июля 2002 года, а также в отношении ЮЛ зарегистрированных на территории Республики Крым или территории города федерального значения Севастополя на день принятия в РФ и образования в составе РФ новых субъектов - Республики Крым и города федерального значения Севастополя (значение реквизита «КодСпОбрЮЛ» в элементе «СпОбрЮЛ» =«03»)
Не формируется в отношении российских организаций, созданных с 1 июля 2002 года (значение реквизита «КодСпОбрЮЛ» в элементе «СпОбрЮЛ» больше «03»)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвРегОрг">
								<xs:annotation>
									<xs:documentation>Сведения о регистрирующем органе по месту нахождения юридического лица</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="КодНО" type="fnst:SONOType" use="required">
										<xs:annotation>
											<xs:documentation>Код органа по справочнику СОУН</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="НаимНО" type="fnst:string-255" use="required">
										<xs:annotation>
											<xs:documentation>Наименование регистрирующего (налогового) органа</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="АдрРО" type="fnst:string-255" use="optional">
										<xs:annotation>
											<xs:documentation>Адрес регистрирующего органа</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвСтатус" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о состоянии (статусе) юридического лица</xs:documentation>
									<xs:documentation>Формируется только в случае, если:
• Регистрация ЮЛ признана недействительной (код статуса по справочнику СЮЛСТ = «701», «702» или «801»)
• ЮЛ находится в процессе ликвидации (код статуса по справочнику СЮЛСТ = «101»);
• ЮЛ находится в процессе реорганизации (код статуса по справочнику СЮЛСТ больше «120» и меньше «200»);
• в отношении ЮЛ в ЕГРЮЛ содержатся следующие сведения (не отмененные и не признанные недействительными):
 - в ЕГРЮЛ внесена запись о принятом решении о предстоящем исключении недействующего ЮЛ из ЕГРЮЛ  (код статуса по справочнику СЮЛСТ = «105»)
- ЮЛ находится в процессе уменьшения уставного капитала (код статуса по справочнику СЮЛСТ = «111»);
-  ЮЛ принято решение об изменении места нахождения (код статуса по справочнику СЮЛСТ = «112»).
В случае если в отношении ЮЛ внесено несколько статусов с указанными кодами (например, ЮЛ находится в состоянии нескольких реорганизаций), то формируются сведения по каждому из этих статусов</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвСтатус">
											<xs:annotation>
												<xs:documentation>Сведения о правоспособности (статусе) юридического лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="КодСтатусЮЛ" type="fnst:string-3" use="required">
													<xs:annotation>
														<xs:documentation>Код статуса юридического лица по справочнику СЮЛСТ</xs:documentation>
														<xs:documentation> Может принимать следующие значения: 101, 105, 111, 112, больше «120» и меньше «200», 701, 702, 801</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="НаимСтатусЮЛ" type="fnst:string-500" use="required">
													<xs:annotation>
														<xs:documentation>Наименование статуса юридического лица по справочнику СЮЛСТ</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="СвРешИсклЮЛ" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о решении о предстоящем исключении недействующего ЮЛ из ЕГРЮЛ и его публикации </xs:documentation>
												<xs:documentation>Формируется только в случае, если передается код статуса ЮЛ по справочнику СЮЛСТ=105</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="ДатаРеш" use="required">
													<xs:annotation>
														<xs:documentation>Дата решения</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:date"/>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НомерРеш" type="fnst:string-255" use="required">
													<xs:annotation>
														<xs:documentation>Номер решения</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="ДатаПубликации" use="required">
													<xs:annotation>
														<xs:documentation>Дата публикации решения </xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:date">
															<xs:minInclusive value="2005-07-06"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НомерЖурнала" type="fnst:string-50">
													<xs:annotation>
														<xs:documentation>Номер журнала, в котором опубликовано решение</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
												<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвПрекрЮЛ" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения о прекращении юридического лица</xs:documentation>
									<xs:documentation>Формируется только в случае, если код статуса ЮЛ по справочнику СЮЛСТ больше 200 и меньше 700</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СпПрекрЮЛ">
											<xs:annotation>
												<xs:documentation>Способ прекращения юридического лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="КодСпПрекрЮЛ" type="fnst:string-3" use="required">
													<xs:annotation>
														<xs:documentation>Код способа прекращения по справочнику СЮЛПД</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="НаимСпПрекрЮЛ" type="fnst:string-255" use="required">
													<xs:annotation>
														<xs:documentation>Наименование способа прекращения</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="СвРегОрг" type="fnst:СвРегОргТип">
											<xs:annotation>
												<xs:documentation>Сведения о регистрирующем (налоговом) органе, внесшем запись о прекращении юридического лица</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ДатаПрекрЮЛ" use="required">
										<xs:annotation>
											<xs:documentation>Дата прекращения юридического лица</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date"/>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвУчетНО" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения об учете в налоговом органе</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:annotation>
											<xs:documentation/>
										</xs:annotation>
										<xs:element name="СвНО" type="fnst:СвНОТип">
											<xs:annotation>
												<xs:documentation>Сведения о налоговом органе, в котором юридическое лицо состоит (для ЮЛ, прекративших деятельность - состояло) на учете</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
												<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ИНН" type="fnst:LegalPersonINNType" use="required">
										<xs:annotation>
											<xs:documentation>ИНН юридического лица</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="КПП" type="fnst:KPPType" use="required">
										<xs:annotation>
											<xs:documentation>КПП юридического лица</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаПостУч" use="required">
										<xs:annotation>
											<xs:documentation>Дата постановки на учет в налоговом органе</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date"/>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвРегПФ" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения о регистрации юридического лица в качестве страхователя в территориальном органе Пенсионного фонда Российской Федерации</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвОргПФ">
											<xs:annotation>
												<xs:documentation>Сведения о территориальном органе Пенсионного фонда Российской Федерации</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="КодПФ" type="fnst:STOPFType" use="required">
													<xs:annotation>
														<xs:documentation>Код по справочнику СТОПФ</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="НаимПФ" type="fnst:string-255" use="required">
													<xs:annotation>
														<xs:documentation>Наименование</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи, содержащей указанные сведения</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНДатаИспр" type="fnst:ГРНДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата внесения в ЕГРЮЛ записи об исправлении технической ошибки в указанных сведениях </xs:documentation>
												<xs:documentation>Отсутствует, если в указанных сведениях не исправлялись технические ошибки </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="РегНомПФ" type="fnst:string-20" use="required">
										<xs:annotation>
											<xs:documentation>Регистрационный номер в территориальном органе Пенсионного фонда Российской Федерации</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаРег" use="required">
										<xs:annotation>
											<xs:documentation>Дата регистрации юридического лица в качестве страхователя</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date"/>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвРегФСС" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения о регистрации юридического лица в качестве страхователя в исполнительном органе Фонда социального страхования Российской Федерации</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвОргФСС">
											<xs:annotation>
												<xs:documentation>Сведения об  исполнительном органе Фонда социального страхования Российской Федерации</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="КодФСС" type="fnst:digits-4" use="required">
													<xs:annotation>
														<xs:documentation>Код по справочнику СТОФСС</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="НаимФСС" type="fnst:string-255" use="required">
													<xs:annotation>
														<xs:documentation>Наименование</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
